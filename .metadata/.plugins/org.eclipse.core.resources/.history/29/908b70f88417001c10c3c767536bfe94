	public static void main(String[] args) {
		
		String studentData = "A:201,B:202,C:203,D:204,E:205";
		String[] indData = studentData.split(",");
		String[] data="";
		for(int i=0;i<indData.length;i++){
			System.out.println(indData[i]);
			
		}

	}

}
